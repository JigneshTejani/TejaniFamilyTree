    "resolve-url-loader": "3.1.1",
    "sass-loader": "^14.2.1",
    "semver": "6.3.0",
    "style-loader": "0.23.1",
    "styled-components": "^6.1.8",
    "terser-webpack-plugin": "2.3.5",
